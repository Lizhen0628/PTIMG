  <script type="text/javascript">
    $(document).ready(function() {
        $('input[name="passid"]').val(0);
        $('.login input[type="submit"]').click(function() {
            var val = $('.login div.active input[name="username"]').val();
            if ('' == val) {
                alert("Username can not be empty!");
                return false;
            }
            var val = $('.login div.active input[name="password"]').val();
            if ('' == val) {
                alert("Password can not be empty!");
                return false;
            }
        });

        $('.dropdown-menu-menu').click(function() {
            var menus = new Array(["安全提问（未设置请忽略）","什么时候开始玩PT的","通过什么途径知道TTG的","你在电影院看的第一部电影的名称","你最喜欢电影的名称","你最喜欢AV女优的名称","你最喜欢演员的名称","你最期待电影的名称"],
