        $(".todo").click(function() {
            $(this).toggleClass("todo-done");
            if ($(this).hasClass('todo-done')) {
                $(this).find("input[name=rememberme]").val("yes");
            } else {
                $(this).find("input[name=rememberme]").val("no");
            }
        });

        var $form_login = $('div.login'),
        $currentForm = $form_login.children('div.active'),
        $linkForm = $('div').find('.linkform');

        $form_login.children('div').each(function(i){
            var $theForm = $(this);
            if(!$theForm.hasClass('active'))
                $theForm.hide();
        });

        $linkForm.bind('click',function(e){
            var $link = $(this);
            var target = $link.attr('rel');
            $currentForm.fadeOut(0,function(){
                $currentForm.removeClass('active');
                $currentForm= $form_login.children('div.'+target);
                $form_login.stop().animate({
                    width  : $currentForm.data('width') + 'px',
                    height : $currentForm.data('height') + 'px'
                },0,function(){
                    $currentForm.addClass('active');
                    $currentForm.fadeIn(0);
                    $("button#b_language").html($link.text()+"<span class=\"caret\"></span>");
                });
            });
            e.preventDefault();
        });
    });
